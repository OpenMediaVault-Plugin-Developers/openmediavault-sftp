[{
	"type": "rpc",
	"id": "rpc.sftp.setsettings",
	"params": {
		"type": "object",
		"properties": {
			"enable": {
				"type": "boolean",
				"required": true
			},
			"port":{
				"type": "integer",
				"minimum": 1,
				"maximum": 65535,
				"required": true
			},
			"passwordauthentication": {
				"type": "boolean",
				"required": true
			},
			"pubkeyauthentication": {
				"type": "boolean",
				"required": false
			},
			"allowgroups": {
				"type": "boolean",
				"required": false
			},
			"extraoptions": {
				"type": "string",
				"required": true
			}
		}
	}
},{
	"type": "rpc",
	"id": "rpc.sftp.setshare",
	"params": {
		"type": "object",
		"properties": {
			"uuid": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"sharedfolderref": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"username": {
				"type": "string",
				"default": ""
			}
		}
	}
}]
